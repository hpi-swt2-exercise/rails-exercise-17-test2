<h1>Papers</h1>
 
<%= link_to 'Add paper', new_paper_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Venue</th>
    <th>Year</th>
    <th colspan="3"></th>
  </tr>
 
 <% @papers.each do |paper| %>
    <tr>
      <td><%= paper.title %></td>
      <td><%= paper.venue %></td>
      <td><%= paper.year %></td>
      <td><%= link_to 'Show', paper_path(paper) %></td>
    </tr>
  <% end %>
</table>