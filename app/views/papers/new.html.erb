<h1>New Paper</h1>

<%= form_for :paper, url: papers_path do |f| %>

  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :venue %><br>
    <%= f.text_field :venue %>
  </p>  

  <p>
    <%= f.label :year %><br>
    <%= f.text_field :year %>
  </p>  

  <p>
    <%= f.submit %>
  </p>

<% end %>